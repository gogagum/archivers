    "version": "0.2.0",
    "configurations": [
        {
            "type": "cppdbg",
            "request": "launch",
            "name": "Debug tests.",
            "program": "${workspaceRoot}/build/encoding_lib/test/archiever_tests",
            "cwd": "${workspaceRoot}"
        },
        {
            "name": "ppma encoder debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/build/ppma_archiever/ppma_encoder",
            "args": ["-i", "${workspaceRoot}/testing/dataset/pic", "-b", "8", "-c", "4"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
